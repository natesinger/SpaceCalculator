Create GmatFunction TargeterInsideFunction;
GMAT TargeterInsideFunction.FunctionPath = 'C:\Users\rian2\Desktop\CTFs\Projects\HackASat\SpaceCalculator\TargeterInsideFunction.gmf';


%----------------------------------------
%---------- Mission Sequence
%----------------------------------------

BeginMissionSequence;
Global 'Make Objects Global' DefaultDC EarthView NearEarth_ForceModel MainTank MAVEN MOI ReportFile1 SolarSystemView TCM;
